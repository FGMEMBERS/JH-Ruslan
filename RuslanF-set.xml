<PropertyList>

	<sim>

		<description>Antonov An-124 Ruslan Freighter</description>
		<author>Peter Brendt (JWocky) - FDM, Autopilot, Systems</author>
		<author>Andino Gonthaeler - Model taken from the A-124 under Yasim</author>

		<aircraft-version>V03.00.00</aircraft-version>
		<minimum-fg-version>2018.2.1</minimum-fg-version>
		<status>pre-beta</status>

		<aircraft-variant>
			<aircraft-type type="string">freighter</aircraft-type>
			<aircraft-subtype type="string">RuslanF</aircraft-subtype>
		</aircraft-variant>
		<tanker type="bool">false</tanker>

		<rating>
			<FDM type="int">0</FDM>
			<systems type="int">0</systems>
			<cockpit type="int">0</cockpit>
			<model type="int">1</model>
		</rating>

		<flight-model>jsb</flight-model>
		<aero>Aero/RuslanF_aero</aero>

		<!-- Splash Screens -->
		<startup>
			<splash-texture>Splashes/splash1.jpg</splash-texture>
		</startup>

		<!-- Settings for Model -->
		<model>

			<!-- path to model file -->
			<path>Models/Ruslan-F.xml</path>

			<livery>
				<file type="string">Antonov</file>
			</livery> 

			<!-- JPack: push forces for Voodoomaster Pushback -->
			<pushback>
				<kp type="double">4000</kp>
				<ki type="double">150</ki>
				<kd type="double">0</kd>
				<position-norm type="double">0</position-norm>
				<enabled type="bool">false</enabled>
			</pushback>

		</model>

		<!-- subsection for submodels if exists -->

		<!-- basic startup settings for sim -->
		<freeze>
			<fuel>false</fuel>	<!-- false is default, only here for quick changes for testing -->
		</freeze>

		<gui>
			<dialogs>
				<autopilot>
					<!-- speed in kts for SYS7/8/9 APs -->
					<kts-select type="bool">true</kts-select>
					<mach-select tpye="bool">false</mach-select>
				</autopilot>
			</dialogs>
		</gui>

		<chase-distance-m>-100</chase-distance-m>

		<sound>
			<path>Sounds/Ruslan_sound.xml</path>
			<ovolume type="float">1.0</ovolume>
			<ivolume type="float">1.0</ivolume>
		</sound>

		<autostart>
			<started>0</started>
		</autostart>

		<virtual-cockpit archive="y">true</virtual-cockpit>

		<instrumentation>
			<path>Systems/instrumentation.xml</path>
		</instrumentation>

		<!-- Multiplay section if exists -->

